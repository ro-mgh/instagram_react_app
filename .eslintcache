        photoURL: \"\",\n        });\n        user.getIdToken(/* forceRefresh */ true).then(async function (idToken) {\n          try {\n            const response = await fetch(\n              \"https://insta-byrm-server.me.uk/user/\" + user.uid,\n              {\n                method: \"put\",\n                headers: {\n                  \"Content-type\": \"application/json\",\n                  Authorization: \"Bearer \" + idToken,\n                },\n                body: JSON.stringify({\n                  avatar: \"\",\n                }),\n              }\n            );\n            if (response.ok) {\n              setRemoving(false);\n              dispatch(exploreUsers());\n            } else {\n              setRemoving(false);\n              dispatch({\n                type: SET_ERROR,\n                payload: {\n                  error: \"Delete avatar: error connecting to DB\",\n                },\n              });\n            }\n          } catch (e) {\n            setRemoving(false);\n            dispatch({\n              type: SET_ERROR,\n              payload: {\n                error: \"Delete avatar: error connecting to DB\",\n              },\n            });\n          }\n        });\n      } else {\n        setRemoving(false);\n        dispatch({\n          type: SET_ERROR,\n          payload: {\n            error: \"Delete avatar: error connecting to firebase\",\n          },\n        });\n      }\n    } catch (e) {\n      setRemoving(false);\n      dispatch({\n        type: SET_ERROR,\n        payload: {\n          error: \"Delete avatar: error connecting to firebase\",\n        },\n      });\n    }\n  };\n\n  return (\n    <div className=\"modal\">\n      <div className=\"modal-wrapper\">\n        <h2 className=\"modal-text\">Change Profile Photo</h2>\n        <label\n          htmlFor=\"upload-photo\"\n          className=\"modal-button image-input-label\"\n        >\n          {isUploading ? (\n            <div className=\"mainfield-progress-wrapper\">\n              <CircularProgress size={20} />\n            </div>\n          ) : (\n            <div>Upload Photo</div>\n          )}\n        </label>\n        <input\n          className=\"image-input\"\n          onChange={uploadAvatar}\n          type=\"file\"\n          accept=\"image/png, image/jpeg\"\n          alt=\"\"\n          id=\"upload-photo\"\n        />\n        <button\n          className=\"modal-button modal-button-remove\"\n          onClick={deleteAvatar}\n        >\n          {isRemoving ? (\n            <div className=\"mainfield-progress-wrapper\">\n              <CircularProgress size={20} />\n            </div>\n          ) : (\n            <div>Remove Current Photo</div>\n          )}\n        </button>\n        <button onClick={toggleModal} className=\"modal-button\">\n          Cancel\n        </button>\n      </div>\n    </div>\n  );\n};\n\nexport default EditAvatar;\n","/Users/rmyagchenkov/Desktop/Insta project/insta_project/src/store/actions/userData.tsx",["438"],"/Users/rmyagchenkov/Desktop/Insta project/insta_project/src/components/main/profile/ProfileInfo.tsx",["439","440"],"/Users/rmyagchenkov/Desktop/Insta project/insta_project/src/components/main/profile/ProfilePictures.tsx",["441","442"],"import React from \"react\";\nimport Gallery from \"../explore/Gallery\";\n\nconst ProfilePictures = (props) => {\n  return (\n    <div className=\"userpicture-wrapper\">\n      <div className=\"userpicture-actions-wrapper\">\n        <div className=\"userpicture-actions-a profilepicture-actions-a\">\n          <div>\n            <svg\n              aria-label=\"Posts\"\n              fill=\"#262626\"\n              height=\"12\"\n              viewBox=\"0 0 48 48\"\n              width=\"12\"\n            >\n              <path\n                clipRule=\"evenodd\"\n                d=\"M45 1.5H3c-.8 0-1.5.7-1.5 1.5v42c0 .8.7 1.5 1.5 1.5h42c.8 0 1.5-.7 1.5-1.5V3c0-.8-.7-1.5-1.5-1.5zm-40.5 3h11v11h-11v-11zm0 14h11v11h-11