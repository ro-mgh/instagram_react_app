\n                        user,\n                      },\n                    });\n                  } else {\n                    user.delete();\n                    return dispatch({\n                      type: SIGNUP_ERROR,\n                      payload: {\n                        authMsgError: \"Error adding new user to DB\",\n                      },\n                    });\n                  }\n                } catch (e) {\n                  user.delete();\n                  return dispatch({\n                    type: SIGNUP_ERROR,\n                    payload: {\n                      authMsgError: \"Error adding new user to DB\",\n                    },\n                  });\n                }\n              })\n              .catch(function (e) {\n                user.delete();\n                return dispatch({\n                  type: SIGNUP_ERROR,\n                  payload: {\n                    authMsgError: \"Error adding new user to DB\",\n                  },\n                });\n              });\n          } else {\n            dispatch({\n              type: SIGNUP_ERROR,\n              payload: {\n                authMsgError:\n                  \"Something went wrong, we couldn't create your account. Please try again.\",\n              },\n            });\n          }\n        })\n        .catch(function (error) {\n          dispatch({\n            type: SIGNUP_ERROR,\n            payload: {\n              authMsgError:\n                \"Something went wrong, we couldn't create your account. Please try again.\",\n            },\n          });\n        });\n    } catch (err) {\n      console.log(err);\n      dispatch({\n        type: SIGNUP_ERROR,\n        payload: {\n          authMsgError:\n            \"Something went wrong, we couldn't create your account. Please try again.\",\n        },\n      });\n    }\n  }\n};\n\n// Signing in with Firebase\nexport const signinUser = (\n  email: string,\n  password: string\n  // ,callback\n): 