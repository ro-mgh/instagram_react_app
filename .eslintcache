 response = await fetch(\n                    \"https://insta-byrm-server.me.uk/user/\" + user.uid,\n                    {\n                      method: \"put\",\n                      headers: {\n                        \"Content-type\": \"application/json\",\n                        Authorization: \"Bearer \" + idToken,\n                      },\n                      body: JSON.stringify({\n                        avatar: data.location,\n                      }),\n                    }\n                  );\n                  if (response.ok) {\n                    dispatch(exploreUsers());\n                    setUploading(false);\n                  } else {\n                    setUploading(false);\n                    dispatch({\n                      type: SET_ERROR,\n                      payload: {\n                        error: \"Uploading avatar: error connecting to DB\",\n                      },\n                    });\n                  }\n                } catch (e) {\n                  setUploading(false);\n                  dispatch({\n                    type: SET_ERROR,\n                    payload: {\n                      error: \"Uploading avatar: error connecting to DB\",\n                    },\n                  });\n                }\n              });\n          } else {\n            setUploading(false);\n            dispatch({\n              type: SET_ERROR,\n              payload: {\n                error: \"Uploading avatar: error connecting to firebase\",\n              },\n            });\n          }\n        })\n        .catch((err) => {\n          setUploading(false);\n          dispatch({\n            type: SET_ERROR,\n            payload: {\n              error: \"Uploading avatar: error connecting to S3\",\n            },\n          });\n        });\n    }\n  };\n\n  const deleteAvatar = (e) => {\n    e.preventDefault();\n    setRemoving(true);\n    ReactS3Client.deleteFile(user.uid)\n      .then((data) => {\n        // console.log(\"Photo deleted\");\n      })\n      .catch((err) => {\n        // console.error(err);\n      });\n    try {\n      const user = firebase.auth().currentUser;\n      if (user) {\n        user.updateProfile({\n          photoURL: \"\",\n        });\n        user.getIdToken(/* forceRefresh */ true).then(async function (idToken) {\n          try {\n            const response = await fetch(\n              \"https://insta-byrm-server.me.uk/user/\" + user.uid,\n              {\n                method: \"put\",\n                headers: {\n                  \"Content-type\": \"application/json\",\n                  Authorization: \"Bearer \" + idToken,\n                },\n                body: JSON.stringify({\n                  avatar: \"\",\n                }),\n              }\n            );\n            if (response.ok) {\n              setRemoving(false);\n              dispatch(exploreUsers());\n            } else {\n              setRemoving(false);\n              dispatch({\n                type: SET_ERROR,\n                payload: {\n                  error: \"Delete avatar: error connecting to DB\",\n                },\n              });\n            }\n          } catch (e) {\n            setRemoving(false);\n            dispatch({\n              type: SET_ERROR,\n              payload: {\n                error: \"Delete avatar: error connecting to DB\",\n              },\n            });\n          }\n        });\n      } else {\n        setRemoving(false);\n        dispatch({\n          type: SET_ERROR,\n          payload: {\n            error: \"Delete avatar: error connecting to firebase\",\n          },\n        });\n      }\n    } catch (e) {\n      setRemoving(false);\n      dispatch({\n        type: SET_ERROR,\n        payload: {\n          error: \"Delete avatar: error connecting to firebase\",\n        },\n      });\n    }\n  };\n\n  return (\n    <div className=\"modal\">\n      <div className=\"modal-wrapper\">\n        <h2 className=\"modal